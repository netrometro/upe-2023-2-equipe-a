generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Alternativa {
  id    Int    @id @default(autoincrement())
  texto String
  letra String
}

model Questao {
  id           Int    @id @default(autoincrement())
  titulo       String @unique
  Alternativas String
  resposta     String
}
